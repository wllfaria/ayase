; Module @ ../samples/module.aya
+const to_add = $0002
; Secondary @ ../samples/secondary.aya
+fill:
JLE &[r1], $0400
MOV &[r8], R7
ADD R8, !to_add
ADD ACC, !to_add
JMP &[!fill]
RET
